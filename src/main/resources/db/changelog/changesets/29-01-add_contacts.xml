<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.29.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1748543995853-1" author="stasp">
        <createTable tableName="contacts">
            <column name="first_history_debt" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="first_current_debt" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="second_history_debt" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="second_current_debt" type="DOUBLE">
                <constraints nullable="false"/>
            </column>
            <column name="first_user_id" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_contacts"/>
            </column>
            <column name="second_user_id" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_contacts"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>